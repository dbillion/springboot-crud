{
  "name": "lightrun",
  "version": "1.3.1",
  "author": "Lightrun platform Inc.",
  "description": "Lightrun Debug Agent for Node.js",
  "main": "./build/src/index",
  "types": "./build/src/index.d.ts",
  "repository": "lightrun/lightrun",
  "keywords": [
    "lightrun",
    "debugger",
    "debug",
    "cloud",
    "cloud debug",
    "cloud debugger"
  ],
  "engines": {
    "node": ">=10"
  },
  "files": [
    "build/src",
    "start.js"
  ],
  "scripts": {
    "prepare": "npm run compile",
    "presystem-test": "npm run compile",
    "system-test": "nyc mocha build/system-test --timeout 600000",
    "test": "nyc cross-env CLOUD_DEBUG_ASSERTIONS=1 mocha build/test",
    "compile": "tsc -p .",
    "postcompile": "cpy src/resources test/*.js test/**/*.js test/fixtures build --parents",
    "fix": "gts fix",
    "pretest": "npm run compile",
    "prepack": "npm run obfuscate",
    "lint": "gts check",
    "docs": "compodoc src/",
    "docs-test": "linkinator docs",
    "predocs-test": "npm run docs",
    "clean": "gts clean",
    "precompile": "gts clean",
    "api-extractor": "api-extractor run --local",
    "api-documenter": "api-documenter yaml --input-folder=temp",
    "obfuscate": "javascript-obfuscator build -o build/ --config obfuscating-options.json"
  },
  "dependencies": {
    "@google-cloud/common": "^3.8.1",
    "acorn": "^8.0.0",
    "coffeescript": "^2.0.0",
    "extend": "^3.0.2",
    "findit2": "^2.2.3",
    "gcp-metadata": "^4.0.0",
    "p-limit": "^3.0.1",
    "semver": "^7.0.0",
    "source-map": "^0.6.1",
    "split": "^1.0.0",
    "gaxios": "^4.2.0",
    "log4js": "^6.3.0",
    "moment": "^2.29.1",
    "memory-streams": "^0.1.3",
    "js-yaml": "^4.1.0",
    "java-properties": "^1.0.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.11",
    "@types/acorn": "^4.0.2",
    "@types/estree": "0.0.45",
    "@types/extend": "^3.0.0",
    "@types/mocha": "^8.0.0",
    "@types/mv": "^2.1.0",
    "@types/ncp": "^2.0.3",
    "@types/node": "~10.17.0",
    "@types/proxyquire": "^1.3.28",
    "@types/semver": "^7.0.0",
    "@types/tmp": "^0.2.0",
    "@types/uuid": "^8.0.0",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.0",
    "execa": "^4.0.0",
    "gts": "^2.0.0",
    "linkinator": "^2.0.0",
    "mocha": "^8.0.0",
    "mv": "^2.1.1",
    "ncp": "^2.0.0",
    "nock": "^13.0.0",
    "nyc": "^15.0.0",
    "proxyquire": "^2.0.0",
    "teeny-request": "^7.0.0",
    "tmp-promise": "^3.0.0",
    "typescript": "^3.8.3",
    "uuid": "^8.0.0",
    "@microsoft/api-documenter": "^7.8.10",
    "@microsoft/api-extractor": "^7.8.10",
    "timekeeper": "^2.2.0",
    "javascript-obfuscator": "^2.14.0"
  }
}
